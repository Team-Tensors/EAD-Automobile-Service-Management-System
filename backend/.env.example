# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control!

# Spring Profile Configuration (optional)
# SPRING_PROFILES_ACTIVE=dev    # For development (default)
# SPRING_PROFILES_ACTIVE=prod   # For production

# Database Configuration
DB_URL=jdbc:postgresql://your-database-host:5432/your-database-name
DB_USERNAME=your-database-username
DB_PASSWORD=your-database-password

# Google OAuth2 Credentials
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Frontend URL
FRONTEND_URL=http://localhost:5173

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-at-least-256-bits
JWT_EXPIRATION=86400000
JWT_REFRESH_EXPIRATION=604800000
JWT_REFRESH_MAX_TOKENS=5

